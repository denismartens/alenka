<% @images.each do |image| %>
	<% if type == :grid || type == :all %>
		<div class='grid-item thumbnail' role='button'>
			<img src=<%= image %>>
		</div>
	<% end %>
	<% if type == :carousel || type == :all %>
		<div class='item carousel-item'>
			<img class="img-responsive" data-src=<%= image.gsub('thumbnail_', '') %>>
		</div>
	<% end %>
<% end %>