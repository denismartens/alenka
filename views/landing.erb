<%= erb :carousel, layout: false, locals: {controls: true}%>
<script type='text/javascript'>
	$(document).ready(function() {
		$all_items = $('.carousel-item');
		$current_item = $all_items.eq(Math.floor(Math.random() * $all_items.length));
		loadImage($current_item, initCarousel($current_item));
	});
</script>